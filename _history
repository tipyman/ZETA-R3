k += 1\n    }\n    let crc_check2 = data_array.concat(array).slice(2)\ncrc16_data2 = crc16(crc_check2)\n    UART_BIN.UART_BIN_TX((crc16_data2 >> 8) & 0xff)\n    UART_BIN.UART_BIN_TX(crc16_data2 & 0xff)\n    for (let index = 0; index < 6; index++) {\n        Query.push(0)\n    }\n    Query = receive_query()\n    if (Query[0] == 0) {\n        Query[3] = 0\n    }\n    pins.digitalWritePin(DigitalPin.P2, 1)\n    return Query[3]\n}\nfunction crc16 (data: any[]) {\n    let crc = 0\n    for (let i = 0; i <= data.length - 1; i++) {\n        crc ^= (data[i] << 8)\nfor (let index = 0; index < 8; index++) {\n            if ((crc & 0x8000) != 0) {\n                crc = (crc << 1) ^ 0x1021\n            } else {\n                crc <<= 1\n            }\n            crc &= 0xFFFF\n        }\n    }\n    return crc\n}\npins.onPulsed(DigitalPin.P8, PulseValue.High, function () {\n    for (let index = 0; index < 16; index++) {\n        Received_data.push(0)\n    }\n    basic.pause(50)\n    Received_data = receive_query()\n    k = 0\n    for (let index = 0; index < 11; index++) {\n        basic.showNumber(Received_data[k])\n        k += 1\n    }\n})\ninput.onButtonPressed(Button.A, function () {\n    温度 = 17\n    明るさ = 34\n    ノイズ = 51\n    if (data_tx([温度, 明るさ, ノイズ]) == 1) {\n        basic.showIcon(IconNames.Happy)\n    } else {\n        basic.showIcon(IconNames.No)\n    }\n})\ninput.onButtonPressed(Button.AB, function () {\n    for (let index = 0; index < 4; index++) {\n        MAC_Address.push(0)\n    }\n    MAC_Address = Inquire_MAC()\n    basic.showNumber(MAC_Address[0])\n    basic.showNumber(MAC_Address[1])\n    basic.showNumber(MAC_Address[2])\n    basic.showNumber(MAC_Address[3])\n})\ninput.onButtonPressed(Button.B, function () {\n    basic.showNumber(Inquire_Module_Status())\n})\nfunction Inquire_MAC () {\n    for (let index = 0; index < 8; index++) {\n        temp.push(0)\n    }\n    temp = command_assert([\n    250,\n    245,\n    3,\n    16\n    ])\n    temp[0] = temp[4]\n    temp[1] = temp[5]\n    temp[2] = temp[6]\n    temp[3] = temp[7]\n    return temp\n}\nfunction receive_query () {\n    for (let index = 0; index < 16; index++) {\n        temp.push(0)\n    }\n    if (UART_BIN.UART_BIN_RX() == 250) {\n        if (UART_BIN.UART_BIN_RX() == 245) {\n            temp[0] = 250\n            temp[1] = 245\n            counter = UART_BIN.UART_BIN_RX()\n            temp[2] = counter\n            k = 3\n            for (let index = 0; index < counter; index++) {\n                temp[k] = UART_BIN.UART_BIN_RX()\n                k += 1\n            }\n        }\n    }\n    return temp\n}\nfunction Inquire_Module_Status () {\n    temp = command_assert([\n    250,\n    245,\n    3,\n    20\n    ])\n    return temp[3]\n}\nlet temp: number[] = []\nlet MAC_Address: number[] = []\nlet ノイズ = 0\nlet 明るさ = 0\nlet 温度 = 0\nlet Received_data: number[] = []\nlet crc16_data2 = 0\nlet data_array: number[] = []\nlet Query: number[] = []\nlet crc16_data = 0\nlet k = 0\nlet Array_length = 0\nlet counter = 0\npins.digitalWritePin(DigitalPin.P2, 1)\ncounter = 0\nserial.redirect(\nSerialPin.USB_TX,\nSerialPin.USB_RX,\nBaudRate.BaudRate115200\n)\n","README.md":"","pxt.json":"{\n    \"name\": \"ZETA-R3\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"core\": \"*\",\n        \"radio\": \"*\",\n        \"microphone\": \"*\",\n        \"ZETag-R2\": \"github:tipyman/pxt-ZETag-r2#75a914c7597ac8a9d0c6b682f986781536be7e66\",\n        \"UART Binary communication\": \"github:tipyman/pxt-uart_binary_communication#d47fb114338f9d92af7ded6d3f6bb3ec11cb6909\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"README.md\"\n    ],\n    \"preferredEditor\": \"tsprj\"\n}\n"}},{"timestamp":1764338157847,"editorVersion":"8.0.18","text":{"main.blocks":"<xml xmlns=\"https://developers.google.com/blockly/xml\"><variables><variable id=\"[ZK`}X|*ceG1ElEfRuY?\">Array_length</variable><variable id=\"{pMouX,u1_L7kIp4dWjG\">k</variable><var